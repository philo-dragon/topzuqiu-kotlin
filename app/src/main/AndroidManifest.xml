<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.com.topzuqiu">

    <application
        android:name="cn.com.topzuqiu.base.ComponentApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TransparentTheme">
        <activity
            android:name="cn.com.topzuqiu.ui.SplashActivity"
            android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.com.topzuqiu.ui.WelcomeActivity" />
        <activity android:name="cn.com.topzuqiu.ui.activity.MainActivity" />
    </application>

</manifest>